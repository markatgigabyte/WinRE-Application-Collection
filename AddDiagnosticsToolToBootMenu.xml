         <Name>Fabrikam Utility</Name>
         <Description>Troubleshoot your Fabrikam PC</Description>
   </WinRETool>
   <WinRETool locale="fr-fr">
      <Name>Utilité de Fabrikam</Name>
      <Description>Dépannez votre PC de Fabrikam</Description>
   </WinRETool>
</BootShell>
